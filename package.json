{
  "name": "programming-helpdesk",
  "description": "helpdesk application",
  "author": "Jon Meyers",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint-config-airbnb": "^16.1.0",
    "pouchdb": "^6.4.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "semantic-ui-css": "2.2.12",
    "semantic-ui-react": "0.78.2"
  },
  "main": "src/electron-starter.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dev": "BROWSER=none nf start",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/images/helpdesk.icns --prune=true --out=release-builds",
    "package-win": "electron-packager ./ programming-helpdesk --overwrite --asar --out=./release-builds --platform=win32 --arch=x64 --prune=true --version-string.FileDescription=swin --version-string.ProductName=\"Programming Helpdesk\" --version-string.CompanyName=swin",
    "package-linux": "electron-packager . programming-helpdesk --overwrite --asar=true --platform=linux --arch=x64 --icon=src/images/helpdesk.icns --prune=true --out=release-builds",
    "package-all": "electron-packager ./ --all --out=release-builds"
  },
  "devDependencies": {
    "electron": "^1.8.2",
    "electron-packager": "^11.0.1",
    "electron-rebuild": "^1.7.3",
    "foreman": "^2.0.0"
  }
}
